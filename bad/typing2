;; nieistniejacy konstruktor
begin
    type t = t(int) | k(bool)
    var x = k(true)
    
    print match x to int with
          s(y,z) -> 9
        | _ -> 8
        end
end

;; typ nie posiadajacy konstruktorow
begin
    type t = t(int) | k(bool)
    
    print match 5 to int with
        ;; typ nie posiada konstruktorow
        t(x) -> 8
        end
end
