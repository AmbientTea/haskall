begin
    ;; type alias / typedef
    alias I2I = (int) => int
    var f : I2I = fun(x: int) : int = 2 * x
    var x = 0
    var y = 4
    
    procedure wypisz(x: int) begin
        print x;
        print "\n"
        x = x + 1000 ;; x zostaje przesloniety
    end return 1
    
    
    while x < (f(y)) do
        wypisz(x)
        x = x+1
    done
    
end
